<prescription xmlns="http://greenify.github.io/schemas/prescription/v3" type="activity" sender="other">
  <intent-filter>
    <action name="android.intent.action.VIEW" />
    <scheme name="market" />
    <ssp prefix="//details?id=com.taobao.taobao" />
    <ssp prefix="//details?id=com.tmall.wireless" />
  </intent-filter>
  <intent-filter>
    <action name="android.intent.action.VIEW" />
    <scheme name="http" />
    <ssp sglob="//market.android.com/.*com.taobao.taobao.*" />
    <ssp sglob="//market.android.com/.*com.tmall.wireles.*" /> <!-- "com.tmall.wireles" (missing one "s") appears in javascript -->
  </intent-filter>
</prescription>
